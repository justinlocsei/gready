{
  "name": "gready",
  "version": "0.0.1",
  "description": "A recommendation engine built on the Goodreads API",
  "scripts": {
    "build": "npm run build:clear && npm run generate-validators && npm run compile",
    "build:clear": "ts-node ./src/bin/clear-build.ts",
    "compile": "tsc --project tsconfig.build.json --outDir ./dist/",
    "generate-validators": "ts-node ./src/bin/generate-validators.ts",
    "lint": "npm run lint:code && npm run lint:types",
    "lint:code": "eslint ./bin/* ./src/ ./tests/ --ext .ts; true",
    "lint:types": "tsc --noEmit; true",
    "test": "mocha -r ts-node/register --file ./tests/helpers/mocha.ts './tests/**/*.test.ts'",
    "test:clear-fixtures": "ts-node ./src/bin/clear-test-fixtures.ts",
    "test:coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinlocsei/gready.git"
  },
  "author": "Justin Locsei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justinlocsei/gready/issues"
  },
  "homepage": "https://github.com/justinlocsei/gready#readme",
  "bin": {
    "gready": "dist/bin/gready.js"
  },
  "dependencies": {
    "ajv": "^6.11.0",
    "async": "^3.1.1",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.15",
    "superagent": "^5.2.1",
    "xml2js": "^0.4.23",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/async": "^3.0.7",
    "@types/chai": "^4.2.8",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cheerio": "^0.22.16",
    "@types/fs-extra": "^8.0.1",
    "@types/glob": "^7.1.1",
    "@types/graceful-fs": "^4.1.3",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.0",
    "@types/sinon": "^7.5.1",
    "@types/superagent": "^4.1.4",
    "@types/tmp": "^0.1.0",
    "@types/xml2js": "^0.4.5",
    "@types/yargs": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-cli": "^1.1.1",
    "mocha": "^7.0.1",
    "nock": "^11.7.2",
    "nyc": "^15.0.0",
    "sinon": "^8.1.1",
    "source-map-support": "^0.5.16",
    "tmp": "^0.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "typescript-json-schema": "^0.42.0"
  }
}
